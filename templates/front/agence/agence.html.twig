							<div
								class="col_25 ractangle_box_cover">
								
								<div class="full_width ">
									<img src="{{ asset('uploads/images/agences/logo/' ~ agence.brochureFilename) }}" alt="house">
									{# <div class="inner_ovelay">
										<h4>{{agence.name}}</h4>
										
									</div> #}
								</div>

								<div class="circle_icon">
									<i class="fa fa-arrow-right small_circle"></i>
								</div>

							</div>
						</a>
						<!-- first ractangle End -->
					{% endfor %}

				</div>

			</div>
		</div>
	</div>
	<!-- counter section End -->
	<!-- feature section start -->
	<!-- slider_tab_main end -->



{% endblock %}
