{% extends 'front/home.html.twig' %}
{% block avion %}{% endblock %}
{% block top_menu %}

		<!--page title start-->
	<div class="page_title" data-stellar-background-ratio="0" data-stellar-vertical-offset="0" style="background-image:url({{asset("/front/assets/images/Home/home4.jpg")}});">
		<ul>
			<li><a href="javascript:;">Agencies</a></li>
		</ul>
	</div>
	<!--page title end-->
	<div class="clearfix"></div>


{% endblock %}


{% block content %}


	<!-- slider_tab_main end -->
	<div class="full_width travelite_world_section">
		<div class="container">
			<div class="row">

				<div class="heading_team">
					<h3>OUR PARTNERS</h3>
