{% extends 'front/home.html.twig' %}
{% block avion %}{% endblock %}

{% block top_menu %}

		<!--page title start-->
	<div class="page_title" data-stellar-background-ratio="0" data-stellar-vertical-offset="0" style="background-image:url({{asset("/front/assets/images/Home/home4.jpg")}});">
		<ul>
			<li><a href="javascript:;">Good &nbsp address &nbsp Details &nbsp for &nbsp{{goodAddress.intitule}}</a></li>
		</ul>
	</div>
	<!--page title end-->
	<div class="clearfix"></div>


{% endblock %}

{% block content %}
	<!-- traveller story section start -->
	<section class="full_width ds_traveller_story">
		<div class="container">
			<div class="row">
				<div class="col-lg-5 col-md-5 col-sm-8 col-xs-8">
					<div class="left_side_slider">

						{# <ul class="about_slider">
							<li><img src="{{ asset('uploads/images/goodAddress/picture/' ~ goodAddress.picture) }}" alt=""></li>
							{% for image in goodAddress.images %}
								<li><img src="{{ asset('uploads/images/goodAddress/pictures/' ~ image) }}" alt=""></li>
							{% endfor %}

						</ul> #}

							<!-- slider start -->
								<div class="hotel_thumbnail_slider">
									<ul class="thumbnail_hotel_slider">
										{% for image in goodAddress.images %}
											<li><img src="{{ asset('uploads/images/goodAddress/pictures/' ~ image) }}" alt="slide"></li>
										{% endfor %}
									</ul>

									<div id="bx-pager" class="hotel_thumbnail_crousel">
										{% for image in goodAddress.images %}
											<a data-slide-index="{{ loop.index0 }}" href=""><img src="{{ asset('uploads/images/goodAddress/pictures/' ~ image) }}" alt="slide"></a>
										{% endfor %}
									</div>
								</div>

								<!-- slider end -->

					</div>
				</div>
				<div class="col-lg-7 col-md-7 col-sm-12 travelite_about_right_side">
					<div class="right_side_details">
						<h3>Good address :
							<b class="traveller_text_b">{{goodAddress.intitule}}</b>
						</h3>
						<h5>
							<i class="icon-location4 mr-1">&nbsp
								{{goodAddress.region.intitule}}</i>
						</h5>
						<p>
							{{goodAddress.description|raw}}
						</p>
					</div>
					<div
						class="full_width check_lists">

					</div>
					

				</div>
			</div>

		</div>

	</section>
	<!-- traveller story section End -->


{% endblock %}

